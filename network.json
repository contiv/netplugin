{
	"name": "contivModel",
	"objects": [
		{
			"name": "network",
			"type": "object",
			"key": [ "tenantName", "networkName"],
			"properties": {
				"networkName": {
					"type": "string",
					"title": "Network name",
					"length": 64,
					"showSummary": true
				},
				"tenantName": {
					"type": "string",
					"title": "Tenant Name",
					"length": 64
				},
				"nwType": {
					"type": "string",
					"format": "^(infra|data)$",
					"default": "\"data\"",
					"title": "Network Type",
					"showSummary": true
				},
				"encap": {
					"type": "string",
					"format": "^(vlan|vxlan)$",
					"title": "Encapsulation",
					"showSummary": true
				},
				"pktTag": {
					"type": "int",
					"title": "Vlan/Vxlan Tag",
					"showSummary": true
				},
				"subnet": {
					"type": "string",
					"format": "^((25[0-5]|2[0-4][0-9]|1[0-9][0-9]|[1-9]?[0-9])(\\\\.(25[0-5]|2[0-4][0-9]|1[0-9][0-9]|[1-9]?[0-9])){3})(\\\\-(25[0-5]|2[0-4][0-9]|1[0-9][0-9]|[1-9]?[0-9]))?/(3[0-1]|2[0-9]|1[0-9]|[1-9])$",
					"title": "Subnet",
					"showSummary": true
				},
				"gateway": {
					"type": "string",
					"format": "^((25[0-5]|2[0-4][0-9]|1[0-9][0-9]|[1-9]?[0-9])(\\\\.(25[0-5]|2[0-4][0-9]|1[0-9][0-9]|[1-9]?[0-9])){3})?$",
					"title": "Gateway",
					"showSummary": true
				},
				"ipv6Subnet": {
					"type": "string",
					"format": "^((((([0-9]|[a-f]|[A-F]){1,4})((\\\\:([0-9]|[a-f]|[A-F]){1,4}){7}))|(((([0-9]|[a-f]|[A-F]){1,4}\\\\:){0,6}|\\\\:)((\\\\:([0-9]|[a-f]|[A-F]){1,4}){0,6}|\\\\:)))/(1[0-2][0-7]|[1-9][0-9]|[1-9]))$",
					"title": "IPv6Subnet",
					"showSummary": true
				},
				"ipv6Gateway": {
					"type": "string",
					"format": "^(((([0-9]|[a-f]|[A-F]){1,4})((\\\\:([0-9]|[a-f]|[A-F]){1,4}){7}))|(((([0-9]|[a-f]|[A-F]){1,4}\\\\:){0,6}|\\\\:)((\\\\:([0-9]|[a-f]|[A-F]){1,4}){0,6}|\\\\:)))?$",
					"title": "IPv6Gateway",
					"showSummary": true
				}
			},
			"link-sets": {
				"services": {
					"ref": "service"
				},
				"appProfiles": {
					"ref": "appProfile"
				},
				"endpointGroups": {
					"ref": "endpointGroup"
				}
			},
			"links": {
				"tenant": {
					"ref": "tenant"
				}
			}
		}
	]
}
