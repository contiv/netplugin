{
	"name": "contivModel",
	"objects": [
		{
			"name": "network",
			"version": "v1",
			"type": "object",
			"key": [ "tenantName", "networkName"],
			"cfgProperties": {
				"networkName": {
					"type": "string",
					"title": "Network name",
					"length": 64,
					"format": "^(([a-zA-Z0-9]|[a-zA-Z0-9][a-zA-Z0-9\\\\-]*[a-zA-Z0-9])\\\\.)*([A-Za-z0-9]|[A-Za-z0-9][A-Za-z0-9\\\\-]*[A-Za-z0-9])$",
					"showSummary": true
				},
				"tenantName": {
					"type": "string",
					"title": "Tenant Name",
					"length": 64,
					"format": "^(([a-zA-Z0-9]|[a-zA-Z0-9][a-zA-Z0-9\\\\-]*[a-zA-Z0-9])\\\\.)*([A-Za-z0-9]|[A-Za-z0-9][A-Za-z0-9\\\\-]*[A-Za-z0-9])$"
				},
				"nwType": {
					"type": "string",
					"format": "^(infra|data)$",
					"default": "data",
					"title": "Network Type",
					"showSummary": true
				},
				"encap": {
					"type": "string",
					"format": "^(vlan|vxlan)$",
					"title": "Encapsulation",
					"showSummary": true
				},
				"pktTag": {
					"type": "int",
					"title": "Vlan/Vxlan Tag",
					"showSummary": true,
					"max": 16777216
				},
				"subnet": {
					"type": "string",
					"format": "^((25[0-5]|2[0-4][0-9]|1[0-9][0-9]|[1-9]?[0-9])(\\\\.(25[0-5]|2[0-4][0-9]|1[0-9][0-9]|[1-9]?[0-9])){3})(\\\\-((25[0-5]|2[0-4][0-9]|1[0-9][0-9]|[1-9]?[0-9])(\\\\.(25[0-5]|2[0-4][0-9]|1[0-9][0-9]|[1-9]?[0-9])){3}))?/(3[0-1]|2[0-9]|1[0-9]|[1-9])$",
					"title": "Subnet",
					"showSummary": true
				},
				"gateway": {
					"type": "string",
					"format": "^((25[0-5]|2[0-4][0-9]|1[0-9][0-9]|[1-9]?[0-9])(\\\\.(25[0-5]|2[0-4][0-9]|1[0-9][0-9]|[1-9]?[0-9])){3})?$",
					"title": "Gateway",
					"showSummary": true
				},
				"ipv6Subnet": {
					"type": "string",
					"format": "^((((([0-9]|[a-f]|[A-F]){1,4})((\\\\:([0-9]|[a-f]|[A-F]){1,4}){7}))|(((([0-9]|[a-f]|[A-F]){1,4}\\\\:){0,6}|\\\\:)((\\\\:([0-9]|[a-f]|[A-F]){1,4}){0,6}|\\\\:)))/(1[0-2][0-7]|[1-9][0-9]|[1-9]))?$",
					"title": "IPv6Subnet",
					"showSummary": true
				},
				"ipv6Gateway": {
					"type": "string",
					"format": "^(((([0-9]|[a-f]|[A-F]){1,4})((\\\\:([0-9]|[a-f]|[A-F]){1,4}){7}))|(((([0-9]|[a-f]|[A-F]){1,4}\\\\:){0,6}|\\\\:)((\\\\:([0-9]|[a-f]|[A-F]){1,4}){0,6}|\\\\:)))?$",
					"title": "IPv6Gateway",
					"showSummary": true
				},
				"cfgdTag": {
					"type": "string",
					"title": "Configured Network Tag",
					"length": 128,
					"format": "^((([a-zA-Z0-9]|[a-zA-Z0-9][a-zA-Z0-9\\\\-]*[a-zA-Z0-9])\\\\.)*([A-Za-z0-9]|[A-Za-z0-9][A-Za-z0-9\\\\-]*[A-Za-z0-9]))?$"
				}
			},
			"operProperties": {
				"pktTag": {
					"type": "int",
					"title": "internal packet tag"
				},
				"externalPktTag": {
					"type": "int",
					"title": "external packet tag"
				},
				"numEndpoints": {
					"type": "int",
					"title": "external packet tag"
				},
				"allocatedAddressesCount": {
					"type": "int",
					"title": "Vlan/Vxlan Tag"
				},
				"allocatedIPAddresses": {
					"type": "string",
					"title": "allocated IP addresses"
				},
				"availableIPAddresses": {
					"type": "string",
					"title": "Available IP addresses"
				},
				"endpoints": {
					"type": "array",
					"items": "endpoint",
					"title": "endpoints in the network"
				},
				"networkTag": {
					"type": "string",
					"title": "Derived Network Tag"
				}
			},
			"link-sets": {
				"services": {
					"ref": "service"
				},
				"endpointGroups": {
					"ref": "endpointGroup"
				},
                "servicelbs":{
					"ref": "servicelb"
                }
			},
			"links": {
				"tenant": {
					"ref": "tenant"
				}
			}
		}
	]
}
