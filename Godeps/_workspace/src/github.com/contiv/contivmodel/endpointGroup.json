{
	"name": "contivModel",
	"objects": [
		{
			"name": "endpointGroup",
			"type": "object",
			"key": [ "tenantName", "groupName" ],
			"properties": {
				"groupName": {
					"type": "string",
					"description": "Endpoint group Name",
					"Title": "Group name",
					"length": 64,
					"format": "^(([a-zA-Z0-9]|[a-zA-Z0-9][a-zA-Z0-9\\\\-]*[a-zA-Z0-9])\\\\.)*([A-Za-z0-9]|[A-Za-z0-9][A-Za-z0-9\\\\-]*[A-Za-z0-9])$",
					"ShowSummary": true
				},
				"tenantName": {
					"type": "string",
					"description": "Tenant Name",
					"Title": "Tenant",
					"length": 64,
					"format": "^(([a-zA-Z0-9]|[a-zA-Z0-9][a-zA-Z0-9\\\\-]*[a-zA-Z0-9])\\\\.)*([A-Za-z0-9]|[A-Za-z0-9][A-Za-z0-9\\\\-]*[A-Za-z0-9])$",
					"ShowSummary": false
				},
				"networkName": {
					"type": "string",
					"Title": "Network",
					"length": 64,
					"format": "^(([a-zA-Z0-9]|[a-zA-Z0-9][a-zA-Z0-9\\\\-]*[a-zA-Z0-9])\\\\.)*([A-Za-z0-9]|[A-Za-z0-9][A-Za-z0-9\\\\-]*[A-Za-z0-9])$",
					"ShowSummary": true
				},
				"policies": {
					"type": "array",
					"items": "string",
					"Title": "Policies",
					"ShowSummary": true
				}
			},
			"link-sets": {
				"services": {
					"ref": "service"
				},
				"policies": {
					"ref": "policy"
				}
			},
			"links": {
				"tenant": {
					"ref": "tenant"
				},
				"network": {
					"ref": "network"
				},
				"appProfile": {
					"ref": "appProfile"
				}
			}
		}
	]
}
